{
  "manifest_version": 3,

  "name": "Let's Cookie!",
  "description": "Cookie Manager",
  "version": "1.0",

  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "background": {
    "service_worker": "js/background.js"
  },

  "permissions": ["cookies", "tabs"],

  "host_permissions": ["<all_urls>"]
}
